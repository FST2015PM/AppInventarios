<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
  <LinearLayout
      xmlns:map="http://schemas.android.com/apk/res-auto"
      android:orientation="vertical"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:gravity="fill_horizontal|fill_vertical"
      android:minHeight="25px"
      android:background="@color/black">
      <RelativeLayout
          android:minWidth="25px"
          android:minHeight="25px"
          android:layout_width="match_parent"
          android:layout_height="70dp"
          android:id="@+id/linearLayout1"
          android:background="@color/green_action_bar"
          android:gravity="center_horizontal">
          <TextView
              android:text="@string/header_text"
              android:layout_width="244.0dp"
              android:layout_height="70.5dp"
              android:id="@+id/header_text"
              android:textStyle="bold"
              android:textSize="18dip"
              android:gravity="center"
              android:textColor="#ffffff"
              android:drawableLeft="@drawable/SenalamientosIcon"
              android:layout_marginLeft="0.0dp"
              android:layout_gravity="center"
              android:layout_margin="5dp"
              android:layout_marginRight="136.5dp" />
      </RelativeLayout>
      <LinearLayout
          android:orientation="horizontal"
          android:background="@color/green_action_bar"
          android:layout_width="match_parent"
          android:layout_height="50dp"
          android:id="@+id/footer">
          <LinearLayout
              android:orientation="vertical"
              android:layout_height="match_parent"
              android:layout_width="0dp"
              android:layout_weight="1"
              android:layout_marginLeft="8dp"
              android:id="@+id/tab1">
              <ImageButton
                  android:id="@+id/tab1_icon"
                  android:layout_weight="2"
                  android:layout_height="0dp"
                  android:layout_width="match_parent"
                  android:background="@color/green_action_bar"
                  android:src="@drawable/PinIcon"
                  android:clickable="true" />
          </LinearLayout>
          <LinearLayout
              android:orientation="vertical"
              android:layout_height="match_parent"
              android:layout_width="0dp"
              android:layout_weight="1"
              android:id="@+id/tab2">
              <ImageButton
                  android:id="@+id/tab2_icon"
                  android:layout_weight="2"
                  android:layout_height="0dp"
                  android:layout_width="match_parent"
                  android:background="@color/green_action_bar"
                  android:src="@drawable/FotoIcon"
                  android:clickable="true" />
          </LinearLayout>
          <LinearLayout
              android:orientation="vertical"
              android:layout_height="match_parent"
              android:layout_width="0dp"
              android:layout_weight="1"
              android:id="@+id/tab3">
              <ImageButton
                  android:id="@+id/tab3_icon"
                  android:layout_weight="2"
                  android:layout_width="match_parent"
                  android:layout_height="0dp"
                  android:background="@color/green_action_bar"
                  android:src="@drawable/OrientacionIcon"
                  android:clickable="true" />
          </LinearLayout>
          <LinearLayout
              android:orientation="vertical"
              android:layout_height="match_parent"
              android:layout_width="0dp"
              android:layout_weight="1"
              android:id="@+id/tab4">
              <ImageButton
                  android:id="@+id/tab4_icon"
                  android:layout_weight="2"
                  android:layout_width="match_parent"
                  android:layout_height="0dp"
                  android:background="@color/green_action_bar"
                  android:src="@drawable/TextoIcon"
                  android:clickable="true" />
          </LinearLayout>
      </LinearLayout>
      <LinearLayout
          android:orientation="vertical"
          android:layout_width="match_parent"
          android:layout_height="match_parent"
          android:gravity="fill_horizontal|fill_vertical"
          android:minHeight="25px">
          <FrameLayout
              android:minWidth="25px"
              android:minHeight="25px"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:id="@+id/containerOrientacionTab">
              <ImageView
                  android:id="@+id/compass"
                  android:src="@drawable/compass"
                  android:layout_width="fill_parent"
                  android:layout_height="fill_parent"
                  android:layout_marginBottom="100dp" />
              <LinearLayout
                  android:layout_width="fill_parent"
                  android:layout_height="fill_parent"
                  android:orientation="vertical"
                  android:gravity="top|center"
                  android:layout_marginTop="20dp"
                  android:layout_marginBottom="120dp">
                  <ImageButton
                      android:id="@+id/SenalOrientSigBtn"
                      android:layout_width="120dp"
                      android:layout_height="37dp"
                      android:background="@drawable/SenalamientosSiguienteBtn" />
              </LinearLayout>
              <FrameLayout
                  android:minWidth="25px"
                  android:minHeight="25px"
                  android:layout_width="match_parent"
                  android:layout_height="match_parent"
                  android:id="@+id/frameLayout1"
                  android:layout_marginTop="320dp">
                  <FrameLayout
                      android:minWidth="25px"
                      android:minHeight="25px"
                      android:layout_width="match_parent"
                      android:layout_height="match_parent"
                      android:id="@+id/frameLayout2"
                      android:layout_marginBottom="40dp">
                      <FrameLayout
                          android:minWidth="25px"
                          android:minHeight="25px"
                          android:layout_width="match_parent"
                          android:layout_height="match_parent"
                          android:id="@+id/frameLayout3"
                          android:layout_marginRight="180dp">
                          <TextView
                              android:textAppearance="?android:attr/textAppearanceLarge"
                              android:layout_width="match_parent"
                              android:layout_height="match_parent"
                              android:id="@+id/txtBrujulaGrados"
                              android:gravity="right|bottom"
                              android:textSize="60dp"
                              android:textStyle="normal"
                              android:text="347Â°" />
                      </FrameLayout>
                      <FrameLayout
                          android:minWidth="25px"
                          android:minHeight="25px"
                          android:layout_width="match_parent"
                          android:layout_height="match_parent"
                          android:id="@+id/frameLayout5"
                          android:layout_marginLeft="190dp"
                          android:layout_marginBottom="45dp">
                          <TextView
                              android:text="N"
                              android:textAppearance="?android:attr/textAppearanceSmall"
                              android:layout_width="match_parent"
                              android:layout_height="match_parent"
                              android:id="@+id/txtBrujulaOrientacion"
                              android:textSize="20dp"
                              android:gravity="bottom" />
                      </FrameLayout>
                      <FrameLayout
                          android:minWidth="25px"
                          android:minHeight="25px"
                          android:layout_width="match_parent"
                          android:layout_height="match_parent"
                          android:id="@+id/frameLayout6"
                          android:layout_marginLeft="190dp"
                          android:layout_marginBottom="23dp"
                          android:layout_marginTop="23dp">
                          <TextView
                              android:text=""
                              android:textAppearance="?android:attr/textAppearanceSmall"
                              android:layout_width="match_parent"
                              android:layout_height="match_parent"
                              android:id="@+id/textView4"
                              android:textSize="20dp"
                              android:gravity="bottom" />
                      </FrameLayout>
                      <FrameLayout
                          android:minWidth="25px"
                          android:minHeight="25px"
                          android:layout_width="match_parent"
                          android:layout_height="match_parent"
                          android:id="@+id/frameLayout8"
                          android:layout_marginLeft="190dp"
                          android:layout_marginTop="45dp">
                          <TextView
                              android:text=""
                              android:textAppearance="?android:attr/textAppearanceSmall"
                              android:layout_width="match_parent"
                              android:layout_height="match_parent"
                              android:id="@+id/textView5"
                              android:textSize="20dp"
                              android:gravity="bottom" />
                      </FrameLayout>
                  </FrameLayout>
                  <FrameLayout
                      android:layout_width="match_parent"
                      android:layout_height="match_parent"
                      android:id="@+id/frameLayout9"
                      android:layout_marginTop="70dp"
                      android:layout_marginRight="200dp">
                      <TextView
                          android:text=""
                          android:textAppearance="?android:attr/textAppearanceLarge"
                          android:layout_width="match_parent"
                          android:layout_height="match_parent"
                          android:id="@+id/txtLatGrados"
                          android:gravity="right|bottom" />
                  </FrameLayout>
                  <FrameLayout
                      android:layout_width="match_parent"
                      android:layout_height="match_parent"
                      android:id="@+id/frameLayout11"
                      android:layout_marginLeft="190dp"
                      android:layout_marginTop="70dp">
                      <TextView
                          android:text=""
                          android:textAppearance="?android:attr/textAppearanceLarge"
                          android:layout_width="match_parent"
                          android:layout_height="match_parent"
                          android:id="@+id/txtLongGrados"
                          android:gravity="left|bottom" />
                  </FrameLayout>
              </FrameLayout>
              <LinearLayout
                  android:layout_width="fill_parent"
                  android:layout_height="fill_parent"
                  android:orientation="vertical"
                  android:gravity="bottom|center"
                  android:layout_marginBottom="45.0dp">
                  <ImageButton
                      android:id="@+id/SenalOrientPopupBtn"
                      android:layout_width="264dp"
                      android:layout_height="69dp"
                      android:background="@drawable/SenalOrientPopupBtn" />
              </LinearLayout>
          </FrameLayout>
      </LinearLayout>
  </LinearLayout>
  <ListView
        android:id="@+id/left_drawer"
        android:layout_width="300dp"
        android:layout_height="match_parent"
        android:layout_gravity="left"
        android:choiceMode="singleChoice"
        android:background="@color/green_menu_lateral"
        android:divider="#FFFFFF"
        android:dividerHeight="3px" />
</android.support.v4.widget.DrawerLayout>